{
  "hosting": {
    "site": "monitorpresence",
    "public": "build/web",
    "ignore": [
      "firebase.json",
      "**/.*"
    ],
    "rewrites": [
      {
        "source": "/api/**",
        "destination": "/proxy-server/proxy.js"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
  
}
